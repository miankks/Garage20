@model IEnumerable<Garage20.Models.Fordon>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
@using (Html.BeginForm())
{
    <p>
        @Html.TextBox("SearchString")
        <input type="submit" value="Sök" />
    </p>
}
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        
        <th>
            @Html.DisplayNameFor(model => model.Medlemmar.FullName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fordonstyper.Typ)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RegNr)
        </th>
        
        <th>
            @Html.DisplayNameFor(model => model.Tid)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.Medlemmar.FullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fordonstyper.Typ)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RegNr)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.Tid)
        </td>
       
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
